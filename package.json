{
  "name": "eslint-config-ponko2",
  "description": "ESLint Shareable Config",
  "version": "22.0.0",
  "author": "Takahito Nakano",
  "dependencies": {
    "globals": "^16.4.0"
  },
  "devDependencies": {
    "@vitest/eslint-plugin": "1.3.23",
    "cross-env": "10.1.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-find-rules": "5.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.5",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "vitest": "3.2.4"
  },
  "files": [
    "base.js",
    "browser.js",
    "index.js",
    "node.js"
  ],
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "MIT",
  "main": "index.js",
  "packageManager": "pnpm@10.18.3",
  "peerDependencies": {
    "eslint": "^9.38.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ponko2/eslint-config-ponko2.git"
  },
  "scripts": {
    "check": "run-p check:*",
    "check:browser": "eslint-config-prettier browser.js",
    "check:index": "eslint-config-prettier index.js",
    "check:node": "eslint-config-prettier node.js",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix --report-unused-disable-directives .",
    "fix:prettier": "prettier --write .",
    "lint": "run-p --aggregate-output -c -l lint:*",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:prettier": "prettier --check .",
    "prepare": "node .husky/install.mjs",
    "test": "cross-env ESLINT_CONFIG_PRETTIER_NO_DEPRECATED=true vitest"
  }
}
